{
  "private": true,
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@spotify/eslint-config-base": "^13.0.1",
    "@spotify/eslint-config-typescript": "^13.0.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "test": "npm run test --workspaces",
    "build": "npm run build --workspaces --if-present",
    "check": "npm run check --workspaces --if-present",
    "lint": "eslint --fix --ext .js --ext .ts --ext .jsx --ext .tsx .",
    "lint:ci": "eslint --ext .js --ext .ts --ext .jsx --ext .tsx .",
    "package": "npm run package --workspaces --if-present"
  },
  "engines": {
    "node": "14 || 16"
  },
  "workspaces": [
    "./packages/*",
    "./internal/*"
  ]
